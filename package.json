{
  "name": "bin-it",
  "version": "0.0.1-test.0",
  "author": "Jeff Dickey @jdxcode",
  "anycli": {
    "commands": "./lib/commands",
    "bin": "bin-it",
    "plugins": [
      "@anycli/plugin-help",
      "@anycli/plugin-not-found",
      "@anycli/plugin-version"
    ]
  },
  "bin": {
    "bin-it": "./bin/run"
  },
  "bugs": "https://github.com/jdxcode/bin-it/issues",
  "dependencies": {
    "@anycli/command": "^1.2.4",
    "@anycli/config": "^1.1.2",
    "@anycli/dev-cli": "^0.1.5",
    "@anycli/plugin-help": "^0.6.0",
    "@anycli/plugin-not-found": "^0.1.15",
    "@octokit/rest": "^14.0.8",
    "axios": "^0.17.1",
    "cli-ux": "^3.3.13",
    "debug": "^3.1.0",
    "execa": "^0.9.0",
    "fs-extra": "^5.0.0",
    "gh-release-assets": "^1.1.1",
    "http-call": "^5.0.2",
    "qqjs": "^0.0.1",
    "shelljs": "^0.8.1",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@anycli/test": "^0.10.9",
    "@anycli/tslint": "^0.2.5",
    "@types/chai": "^4.1.2",
    "@types/execa": "^0.8.1",
    "@types/fs-extra": "^5.0.0",
    "@types/lodash": "^4.14.100",
    "@types/mocha": "^2.2.48",
    "@types/nock": "^9.1.2",
    "@types/node": "^9.4.0",
    "@types/node-notifier": "^0.0.28",
    "@types/read-pkg": "^3.0.0",
    "@types/shelljs": "^0.7.8",
    "@types/write-json-file": "^2.2.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.17.0",
    "eslint-config-anycli": "^1.3.2",
    "mocha": "^5.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".anycli.manifest.json",
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/jdxcode/bin-it",
  "keywords": [
    "anycli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "jdxcode/bin-it",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "concurrently -p command \"eslint .\" \"tsc -p test --noEmit\" \"tslint -p test\"",
    "postpublish": "rm .anycli.manifest.json",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && anycli-dev manifest",
    "test": "echo mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
